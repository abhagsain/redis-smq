{
  "name": "redis-smq",
  "version": "3.0.4",
  "description": "A simple high-performance Redis message queue for Node.js.",
  "author": "Weyoss <weyoss@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "redis",
    "message queue",
    "message",
    "queue",
    "job queue",
    "jobs",
    "redis-smq"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/weyoss/redis-smq.git"
  },
  "homepage": "https://github.com/weyoss/redis-smq",
  "bugs": {
    "url": "https://github.com/weyoss/redis-smq/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/bunyan": "^1.8.7",
    "@types/ioredis": "^4.27.1",
    "@types/lodash": "^4.14.172",
    "@types/redis": "^2.8.31",
    "@types/redlock": "^4.0.2",
    "@types/uuid": "^8.3.1",
    "bunyan": "^1.8.15",
    "cron-parser": "^3.5.0",
    "ioredis": "^4.27.9",
    "lodash": "^4.17.21",
    "neo-async": "^2.6.2",
    "redis": "^3.1.2",
    "redis-smq-monitor": "^2.0.1",
    "redlock": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "bluebird": "^3.7.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "type-coverage": "^2.18.2",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "prebuild": "npm run build:clean",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"{src,tests,types}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --runInBand",
    "type-coverage": "type-coverage --strict --at-least 100 --ignore-files \"dist/**/*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-coverage && npm run build"
    }
  },
  "lint-staged": {
    "*.(ts)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.(json|js)": [
      "prettier --write",
      "git add"
    ]
  },
  "engineStrict": true,
  "engines": {
    "node": ">=7.0.0"
  }
}
